{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"indent": ["error", 4],
		"no-extra-boolean-cast": "error"
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
